{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm:create": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register ./ormconfig.build.ts",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.json",
    "migration:create": "yarn typeorm:cli migration:create",
    "migration:run": "yarn typeorm:cli migration:run",
    "seed:create": "",
    "seed:run": "./node_modules/.bin/typeorm seed:run",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest build --webpack --webpackPath webpack.config.js",
    "start:prod": "node dist/main",
    "start:db": "docker-compose up -d && exit 0",
    "stop:db": "docker-compose kill",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.4.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.1",
    "@nestjs/platform-express": "^7.4.1",
    "@nestjs/typeorm": "^7.1.0",
    "csurf": "^1.11.0",
    "express-rate-limit": "^5.1.3",
    "glob": "^7.1.6",
    "helmet": "^3.23.3",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/testing": "^7.4.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/jest": "26.0.7",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "dotenv": "^8.2.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "26.1.4",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.6",
    "webpack-node-externals": "^2.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
